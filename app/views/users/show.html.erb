<%= render 'shared/header' %>
<div class="wrap">
  <h1 class="user-posts"><%= @user.name %>さんの投稿一覧</h1>
  <ul class="contents">
  <% @posts.each do |post| %>
    <li class="content darkmode-ignore" id="post_<%= post.id %>">
      <%= render "posts/post",post: post %>
    </li>
  <% end %>
  </ul>
  <%= paginate @posts %>
</div>
<script src="https://cdn.jsdelivr.net/npm/darkmode-js@1.5.7/lib/darkmode-js.min.js"></script>